# This file contains a summary of the parametrization for the current Red Hat JBoss EAP instance.

# Golden image used
jboss_eap_golden_image_name: {{ jboss_eap_golden_image_name }}

# Value for the xms and xmx (both are equal)
jvm_xm: {{ jvm_xm }}
jvm_file_encoding: {{jvm_file_encoding}}
jvm_java_io_tmpdir: {{jvm_java_io_tmpdir}}
jvm_user_country: {{jvm_user_country}}
jvm_user_language: {{jvm_user_language}}
jvm_user_timezone: {{jvm_user_timezone}}
jvm_jboss_node_name: {{jvm_jboss_node_name}}

# Name of the separate JBoss EAP instance
jboss_eap_instance_name: {{ jboss_eap_instance_name }}

# Default value for the used standane xml file
jboss_eap_instance_standalone_file: {{ jboss_eap_instance_standalone_file }}

# Application lists
app_list: {{ app_list }}
app_mvn_list: {{ app_mvn_list }}
cli_list: {{ cli_list }}

# Ports
jboss_eap_instance_port_offset: {{ jboss_eap_instance_port_offset }}
jboss_eap_instance_cli_default_port: {{jboss_eap_instance_cli_default_port}}
jboss_eap_instance_cli_port: {{jboss_eap_instance_cli_port}}
